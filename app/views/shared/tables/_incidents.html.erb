<%= render "shared/tables/common", {
  class_name: "incidents-table",
  path: ->(params) { datatable_incidents_path(params) }
}.merge(local_assigns) do %>
<tr>
  <th scope="col">Incident #</th>
  <th scope="col">Occurred</th>
  <th scope="col">District</th>
  <th scope="col">Shooting?</th>
  <th scope="col">Location</th>
  <th scope="col">Street</th>
  <th scope="col">Nature of Incident</th>
  <th scope="col">Offenses</th>
  <th scope="col">Officer(s)</th>
</tr>
<% end %>
