<%= render "shared/tables/common", {
  class_name: "cases-table",
  path: ->(params) { datatable_cases_path(params) }
}.merge(local_assigns) do %>
<tr>
  <th scope="col">Case #</th>
  <th scope="col">Court</th>
  <th scope="col">Date</th>
  <th scope="col">Amount</th>
  <th scope="col">Motor Vehicle</th>
  <th scope="col">Incident Reports</th>
</tr>
<% end %>
