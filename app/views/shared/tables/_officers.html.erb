<%= render "shared/tables/common", {
  class_name: "officers-table",
  path: ->(params) { datatable_officers_path(params) }
}.merge(local_assigns) do %>
<tr>
  <th colspan="7">Employee Info</th>
  <th colspan="3">Activity</th>
  <th colspan="8">Earnings (2019)</th>
</tr>
<tr>
  <th scope="col">Emp #</th>
  <th scope="col">Name</th>
  <th scope="col">Title</th>
  <th scope="col">Start Date</th>
  <th scope="col">Zip</th>
  <th scope="col">State</th>
  <th scope="col">City</th>
  <th scope="col" title="# of IA Cases">Complaints</th>
  <th scope="col" title="Field Interrogation and Observation">FIOs</th>
  <th scope="col" title="# of entries in incident journal">Inc.</th>
  <th scope="col">Total</th>
  <th scope="col">Regular</th>
  <th scope="col">Retro</th>
  <th scope="col">Other</th>
  <th scope="col">Overtime</th>
  <th scope="col">Injured</th>
  <th scope="col" title="Paid details">Detail</th>
  <th scope="col" title="Police Career Incentive Pay Program">Quinn</th>
</tr>
<% end %>
