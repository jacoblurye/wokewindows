<%= render "shared/tables/common", {
  id: id,
  class_name: "complaints-table",
  path: ->(params) { datatable_complaints_path(params) }
}.merge(local_assigns) do %>
<tr>
  <th scope="col">IA #</th>
  <th scope="col">Type</th>
  <th scope="col">Received</th>
  <th scope="col">Occurred</th>
  <th scope="col">Employee</th>
  <th scope="col">Finding</th>
</tr>
<% end %>
